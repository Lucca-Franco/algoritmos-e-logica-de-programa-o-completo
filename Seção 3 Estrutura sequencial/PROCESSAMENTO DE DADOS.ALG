Algoritmo "Processamento de dados"

Var

   x, y, L : inteiro
   c, b1, b2, h, area : Real
   
Inicio

   x <- 5
   y <- 2 * x
   L <- 5
   c <- 2 * L
   b1 <- 6.0
   b2 <- 8.0
   h <- 5.0
   area <- (b1 + b2) / 2.0 * h;
   
   escreval(x)
   escreval(y)
   escreval(L)
   escreval(c)
   escreval(area)

Fimalgoritmo